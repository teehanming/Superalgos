    "definition": {
        "text": "The current Messari API Map accesses the following endpoints of the Messari API.",
        "updated": 1623016798805
    },
    "paragraphs": [
        {
            "style": "Text",
            "text": "Documentation for the Messari API can be found at the following link:",
            "updated": 1623016832972
        },
        {
            "style": "Link",
            "text": "https://messari.io/api/docs->https://messari.io/api/docs",
            "updated": 1623016891013
        },
        {
            "style": "Text",
            "text": "The base request URL is:",
            "updated": 1623016916374
        },
        {
            "style": "Javascript",
            "text": "https://data.messari.io/api/v1/"
        },
        {
            "style": "Title",
            "text": "Mapped Endpoints",
            "updated": 1623017084118
        },
        {
            "style": "Subtitle",
            "text": "Get Asset Metrics",
            "updated": 1623018060417
        },
        {
            "style": "Javascript",
            "text": "assets/{assetKey}/metrics"
